{
  "expo": {
    "name": "Blank Template (TypeScript)",
    "slug": "shop-review-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "extra": {
      "firebase": {
        "apiKey": "your-api-key",
        "authDomain": "your-app.firebaseapp.com",
        "databaseURL": "https://your-app.firebaseio.com",
        "projectId": "your-app",
        "storageBucket": "your-app.appspot.com",
        "messagingSenderId": "your-messageging-sender-id",
        "appId": "your-app-id",
        "measurementId": "your-measurement-id"
      },
      "algolia": {
        "appId": "your-app-id",
        "searchApiKey": "your-search-api-key"
      }
    }
  }
}
